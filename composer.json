{
    "name": "idecardo/nova-telescope",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "telescope"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/telescope": "^1.0",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Idecardo\\NovaTelescope\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idecardo\\NovaTelescope\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
